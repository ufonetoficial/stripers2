{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "npm run build",
    "outputDirectory": "public",
    "framework": null,
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.6.0"
      }
    },
    "routes": [{ "src": "/(css|js|images)/(.*)", "dest": "public/$1/$2" }, { "src": "/(.*)", "dest": "/index.php" }]
  }
  
  
